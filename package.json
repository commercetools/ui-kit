{
  "name": "ui-kit",
  "version": "0.0.0",
  "description": "UI component library based on our Design System",
  "homepage": "https://uikit.commercetools.com/",
  "bugs": "https://github.com/commercetools/ui-kit/issues",
  "private": true,
  "keywords": ["javascript", "frontend", "ui-kit", "design-system", "react"],
  "sideEffects": false,
  "scripts": {
    "auth": "npm_config_registry=https://registry.npmjs.org npm whoami",
    "clean": "manypkg exec rm -rf dist",
    "generate-readmes": "yarn generate-readme --all-workspace-packages",
    "generate-icons": "manypkg run icons generate-icons && manypkg run checkbox-input generate-icons && manypkg run radio-input generate-icons && manypkg run rich-text-utils generate-icons && prettier --write '**/generated/*.tsx'",
    "design-tokens:build": "manypkg run design-system build:tokens",
    "design-tokens:build:watch": "manypkg run design-system build:tokens:watch",
    "build": "./scripts/build.sh",
    "build:watch": "preconstruct watch",
    "start": "yarn --cwd docs start",
    "extract-intl": "formatjs extract --format=./packages/i18n/transifex-transformer.js --out-file=./packages/i18n/data/core.json '{packages,src/components}/**/messages.js'",
    "compile-intl": "yarn workspace @commercetools-uikit/i18n compile-data",
    "lint": "jest --projects jest.eslint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format:js": "prettier --write '**/*.{js,ts,tsx}'",
    "format:md": "prettier --write --parser markdown '**/*.md'",
    "format:yaml": "prettier --write --parser yaml '**/*.yaml'",
    "test": "jest --projects jest.{ts-test,test}.config.js",
    "test:watch": "yarn test --watch",
    "test:bundle": "jest --config jest.bundle.config.js",
    "test:visual": "jest --config jest.visual.config.js --runInBand",
    "vrt:components": "percy exec -- yarn test:visual",
    "bundlesize": "bundlesize",
    "test:bundle:watch": "jest --config jest.bundle.config.js --watch",
    "changeset": "changeset",
    "changeset:version-and-format": "changeset version && yarn prettier --write --parser json '**/package.json' && YARN_ENABLE_IMMUTABLE_INSTALLS=false SKIP_INSTALL_STORYBOOK=true yarn install",
    "visual-testing-app:build": "manypkg run visual-testing-app build",
    "visual-testing-app:build:watch": "manypkg run visual-testing-app build:watch",
    "visual-testing-app:serve": "manypkg run visual-testing-app start",
    "labels:sync": "github-labels sync",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "packageManager": "yarn@3.1.1",
  "workspaces": [
    "design-system",
    "generators/*",
    "packages/*",
    "packages/components/*",
    "packages/components/buttons/*",
    "packages/components/fields/*",
    "packages/components/inputs/*",
    "packages/components/spacings/*",
    "presets/*",
    "visual-testing-app"
  ],
  "preconstruct": {
    "packages": [
      "design-system",
      "packages/*",
      "packages/components/*",
      "packages/components/buttons/*",
      "packages/components/fields/*",
      "packages/components/inputs/*",
      "packages/components/spacings/*",
      "presets/*"
    ]
  },
  "dependencies": {
    "@changesets/changelog-github": "0.4.3",
    "@changesets/cli": "2.21.0",
    "@commercetools-frontend/babel-preset-mc-app": "21.0.0",
    "@commercetools-frontend/eslint-config-mc-app": "21.0.0",
    "@commercetools-local/generator-package-json": "*",
    "@commercetools-local/generator-readme": "*",
    "@commercetools/github-labels": "1.1.0",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@formatjs/cli": "4.8.2",
    "@formatjs/intl-relativetimeformat": "9.5.2",
    "@manypkg/cli": "0.19.1",
    "@manypkg/get-packages": "1.1.3",
    "@percy/cli": "1.0.0-beta.75",
    "@percy/puppeteer": "2.0.0",
    "@preconstruct/cli": "2.1.5",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@svgr/cli": "6.2.1",
    "@svgr/plugin-jsx": "6.2.1",
    "@svgr/plugin-svgo": "6.2.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/react-hooks": "7.0.2",
    "@types/is-hotkey": "^0.1.7",
    "@types/jest": "27.4.0",
    "@types/slate-html-serializer": "^0.6.5",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.3",
    "babel-plugin-typescript-to-proptypes": "1.4.2",
    "browserslist": "4.19.3",
    "bundlesize": "0.18.1",
    "colors": "1.4.0",
    "conventional-changelog-cli": "2.2.2",
    "cross-env": "7.0.3",
    "eslint": "8.9.0",
    "eslint-formatter-pretty": "4.1.0",
    "execa": "6.1.0",
    "formik": "^2.2.9",
    "glob": "7.2.0",
    "global": "4.4.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "intl-pluralrules": "1.3.1",
    "jest": "27.5.1",
    "jest-localstorage-mock": "2.4.19",
    "jest-puppeteer": "6.1.0",
    "jest-runner-eslint": "1.0.0",
    "jest-runner-stylelint": "2.3.7",
    "jest-silent-reporter": "0.5.0",
    "jest-validate": "27.5.1",
    "jest-watch-typeahead": "1.0.0",
    "lint-staged": "12.3.4",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "mri": "1.2.0",
    "omit-empty-es": "1.1.3",
    "patch-package": "6.4.7",
    "postcss": "8.4.6",
    "postcss-syntax": "^0.36.2",
    "postcss-value-parser": "4.2.0",
    "pptr-testing-library": "0.7.0",
    "prettier": "2.5.1",
    "puppeteer": "13.3.2",
    "qs": "6.10.3",
    "rcfile": "1.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.24.6",
    "react-router-dom": "5.3.0",
    "react-test-renderer": "17.0.2",
    "react-value": "0.2.0",
    "replace": "1.2.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.5",
    "stylelint": "14.5.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "4.1.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "tsc-files": "1.1.3",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "@types/unist": "2.0.6",
    "@testing-library/dom": "8.11.3",
    "core-js-compat": "3.21.1",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2"
  }
}
