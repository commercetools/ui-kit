{
  "name": "ui-kit",
  "version": "0.0.0",
  "description": "UI component library based on our Design System",
  "homepage": "https://uikit.commercetools.com/",
  "bugs": "https://github.com/commercetools/ui-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/ui-kit.git"
  },
  "private": true,
  "license": "MIT",
  "keywords": ["javascript", "frontend", "ui-kit", "design-system", "react"],
  "sideEffects": false,
  "scripts": {
    "postinstall": "manypkg check && preconstruct dev && patch-package && yarn --cwd docs",
    "auth": "npm_config_registry=https://registry.npmjs.org npm whoami",
    "clean": "manypkg exec rm -rf dist",
    "generate-readmes": "yarn generate-readme --all-workspace-packages",
    "generate-icons": "manypkg run icons generate-icons && manypkg run checkbox-input generate-icons && manypkg run radio-input generate-icons && manypkg run rich-text-utils generate-icons && prettier --write '**/generated/*.tsx'",
    "design-tokens:build": "manypkg run design-system build:tokens",
    "design-tokens:build:watch": "manypkg run design-system build:tokens:watch",
    "prebuild": "yarn generate-icons && yarn design-tokens:build && yarn i18n:compile",
    "postbuild": "yarn workspace @commercetools-frontend/ui-kit run copy-assets",
    "build": "preconstruct build",
    "build:watch": "preconstruct watch",
    "start": "yarn --cwd docs start",
    "i18n:build": "formatjs extract --format=$(pwd)/packages/i18n/transifex-transformer.js --out-file=$(pwd)/packages/i18n/data/core.json '{packages,src/components}/**/messages.js'",
    "i18n:compile": "manypkg run i18n compile-data",
    "lint": "jest --projects jest.eslint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format:js": "prettier --write '**/*.{js,ts,tsx}'",
    "format:md": "prettier --write --parser markdown '**/*.md'",
    "format:yaml": "prettier --write --parser yaml '**/*.yaml'",
    "pretest": "yarn i18n:compile",
    "test": "jest --projects jest.{ts-test,test}.config.js",
    "test:watch": "yarn test --watch",
    "test:bundle": "jest --config jest.bundle.config.js",
    "test:visual": "jest --config jest.visual.config.js --runInBand",
    "vrt:components": "percy exec -- yarn test:visual",
    "bundlesize": "bundlesize",
    "test:bundle:watch": "jest --config jest.bundle.config.js --watch",
    "changeset": "changeset",
    "changeset:version-and-format": "changeset version && yarn prettier --write --parser json '**/package.json'",
    "visual-testing-app:build": "manypkg run visual-testing-app build",
    "visual-testing-app:build:watch": "manypkg run visual-testing-app build:watch",
    "visual-testing-app:serve": "manypkg run visual-testing-app start",
    "labels:sync": "github-labels sync",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prepare": "husky install"
  },
  "workspaces": {
    "packages": [
      "design-system",
      "generators/*",
      "packages/*",
      "packages/components/*",
      "packages/components/buttons/*",
      "packages/components/fields/*",
      "packages/components/inputs/*",
      "packages/components/spacings/*",
      "presets/*",
      "visual-testing-app"
    ],
    "nohoist": ["react-virtualized/dom-helpers"]
  },
  "preconstruct": {
    "packages": [
      "design-system",
      "packages/*",
      "packages/components/*",
      "packages/components/buttons/*",
      "packages/components/fields/*",
      "packages/components/inputs/*",
      "packages/components/spacings/*",
      "presets/*"
    ]
  },
  "dependencies": {
    "@changesets/changelog-github": "0.4.0",
    "@changesets/cli": "2.16.0",
    "@commercetools-frontend/babel-preset-mc-app": "20.7.0",
    "@commercetools-frontend/eslint-config-mc-app": "20.7.0",
    "@commercetools/github-labels": "1.1.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@formatjs/cli": "4.2.29",
    "@formatjs/intl-relativetimeformat": "9.2.0",
    "@manypkg/cli": "0.18.0",
    "@percy/cli": "1.0.0-beta.60",
    "@percy/puppeteer": "2.0.0",
    "@preconstruct/cli": "2.1.0",
    "@svgr/cli": "5.5.0",
    "@svgr/plugin-jsx": "5.5.0",
    "@svgr/plugin-svgo": "5.5.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.1",
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.2",
    "babel-plugin-typescript-to-proptypes": "1.4.2",
    "browserslist": "4.16.6",
    "bundlesize": "0.18.1",
    "colors": "1.4.0",
    "common-tags": "1.8.0",
    "conventional-changelog-cli": "2.1.1",
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "eslint-formatter-pretty": "4.1.0",
    "formik": "^2.2.9",
    "glob": "7.1.7",
    "global": "4.4.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "intl-pluralrules": "1.2.2",
    "jest": "27.0.6",
    "jest-localstorage-mock": "2.4.14",
    "jest-puppeteer": "5.0.4",
    "jest-runner-eslint": "0.10.1",
    "jest-runner-stylelint": "2.3.7",
    "jest-silent-reporter": "0.5.0",
    "jest-validate": "27.0.6",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.1.1",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "mri": "1.1.6",
    "omit-empty-es": "1.1.3",
    "patch-package": "6.4.7",
    "postcss": "8.3.6",
    "postcss-value-parser": "4.1.0",
    "pptr-testing-library": "0.6.5",
    "prettier": "2.3.2",
    "puppeteer": "10.1.0",
    "qs": "6.10.1",
    "rcfile": "1.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.20.6",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "17.0.2",
    "react-value": "0.2.0",
    "replace": "1.2.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "tsc-files": "1.1.2",
    "typescript": "4.3.5"
  },
  "resolutions": {
    "@types/unist": "2.0.6",
    "@testing-library/dom": "8.1.0",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "**/core-js-compat": "3.15.2"
  }
}
