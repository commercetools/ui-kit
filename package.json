{
  "name": "@commercetools-frontend/ui-kit",
  "version": "1.0.0-beta.30",
  "description": "UI component library based on our Design System",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "src/index.js",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "prepublish": "node ./scripts/generate-icon-exports.js",
    "prebuild": "node ./scripts/generate-base-colors.js && node ./scripts/generate-base-shadows.js && node ./scripts/generate-colors-for-story.js",
    "prestart": "node ./scripts/generate-base-colors.js && node ./scripts/generate-base-shadows.js && node ./scripts/generate-colors-for-story.js",
    "build": "build-storybook -o .public",
    "start": "start-storybook -p 9002",
    "i18n:build": "mc-scripts extract-intl --build-translations --output-path=$(pwd)/i18n 'src/components/**/!(*.spec).js'",
    "lint": "jest --projects jest.eslint.config.js jest.stylelint.config.js",
    "lint:js": "jest --config jest.eslint.config.js",
    "lint:css": "jest --config jest.stylelint.config.js",
    "format": "npm run format:js && npm run format:css & npm run format:md",
    "format:js": "prettier --write '{src,scripts,examples,.storybook}/**/*.js'",
    "format:css": "prettier --parser postcss --write '{src}/**/*.css'",
    "format:md": "prettier --parser markdown --write '{src,examples}/**/*.md'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "common-tags": "1.8.0",
    "dom-helpers": "3.3.1",
    "downshift": "2.2.0",
    "flatpickr": "4.5.1",
    "invariant": "2.2.4",
    "is-touch-device": "1.0.1",
    "lodash.flatmap": "4.5.0",
    "lodash.has": "4.5.2",
    "lodash.isnil": "4.0.0",
    "lodash.omit": "4.5.0",
    "lodash.sortby": "4.7.0",
    "lodash.uniq": "4.5.0",
    "lodash.without": "4.4.0",
    "moment": "2.22.2",
    "moment-timezone": "0.5.21",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-intl": "2.4.0",
    "react-required-if": "1.0.3",
    "react-router-dom": "4.3.1",
    "react-select": "2.0.0",
    "react-textarea-autosize": "7.0.2",
    "react-virtualized": "9.20.0",
    "recompose": "0.26.0",
    "reselect": "3.0.1",
    "styled-components": "3.3.3",
    "warning": "4.0.2"
  },
  "devDependencies": {
    "@commercetools-frontend/babel-preset-mc-app": "1.0.0-beta.30",
    "@commercetools-frontend/eslint-config-mc-app": "1.0.0-beta.30",
    "@commercetools-frontend/jest-preset-mc-app": "1.0.0-beta.30",
    "@commitlint/cli": "7.1.0",
    "@commitlint/config-conventional": "7.1.0",
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-info": "4.0.0-alpha.20",
    "@storybook/addon-knobs": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@storybook/addon-options": "4.0.0-alpha.20",
    "@storybook/react": "4.0.0-alpha.20",
    "@svgr/webpack": "2.2.1",
    "babel-loader": "8.0.2",
    "browserslist": "4.0.2",
    "common-tags": "1.8.0",
    "css": "2.2.3",
    "css-loader": "1.0.0",
    "enzyme": "3.5.0",
    "eslint": "5.3.0",
    "formik": "1.0.2",
    "glob": "7.1.2",
    "html-loader": "0.5.5",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "jest-plugin-filename": "0.0.1",
    "jest-runner-eslint": "0.6.0",
    "jest-runner-stylelint": "1.0.0",
    "lint-staged": "7.2.2",
    "lodash.camelcase": "4.3.0",
    "lodash.mapvalues": "4.6.0",
    "lodash.upperfirst": "4.3.1",
    "markdown-loader": "3.0.0",
    "omit-empty": "0.4.1",
    "postcss": "7.0.2",
    "postcss-color-mod-function": "2.4.3",
    "postcss-import": "12.0.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "5.3.0",
    "postcss-reporter": "5.0.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.14.2",
    "rcfile": "1.0.3",
    "react-dom": "16.4.2",
    "react-value": "0.2.0",
    "storybook-readme": "4.0.0-beta1",
    "style-loader": "0.22.1",
    "stylelint": "9.5.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-order": "0.8.1",
    "svg-url-loader": "2.3.2",
    "thread-loader": "1.2.0",
    "url-loader": "1.1.1"
  }
}
