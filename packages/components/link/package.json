{
  "name": "@commercetools-uikit/link",
  "description": "Links are used either to link to other UI routes, or to link to external pages. This component is a very thin wrapper over either a React Router Link, or a normal HTML <a> tag.",
  "version": "10.39.6",
  "bugs": "https://github.com/commercetools/ui-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/ui-kit.git",
    "directory": "packages/components/link"
  },
  "homepage": "https://uikit.commercetools.com",
  "keywords": [
    "javascript",
    "design system",
    "react",
    "uikit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/link.cjs.js",
  "module": "dist/link.es.js",
  "files": ["dist"],
  "scripts": {
    "prepare": "../../../scripts/version.js replace",
    "prebuild": "rimraf dist",
    "build": "yarn build:bundles",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../../rollup.config.js -i ./src/index.js",
    "build:bundles:watch": "yarn build:bundles -w"
  },
  "dependencies": {
    "@babel/runtime": "7.12.5",
    "@babel/runtime-corejs3": "7.12.5",
    "@commercetools-uikit/design-system": "10.39.6",
    "@commercetools-uikit/utils": "10.39.6",
    "@emotion/core": "^10.0.34",
    "@emotion/styled": "^10.0.27",
    "@emotion/styled-base": "^10.0.31",
    "prop-types": "15.7.2",
    "react-required-if": "1.0.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-router-dom": "5.x",
    "react-intl": "3.x || 4.x || 5.x"
  }
}
