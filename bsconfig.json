{
  "name": "ui-kit",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "bs-dependencies": ["bs-css", "reason-react"],
  "bsc-flags": ["-bs-super-errors"],
  "reason": {
    "react-jsx": 2
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "refmt": 3,
  "gentypeconfig": {
    "language": "untyped",
    "shims": {},
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
