{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "curly": false,
    "no-console": false,
    "no-empty": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "interface-over-type-literal": false,
    "no-namespace": false,
    "ordered-imports": false
  }
}