{
  "name": "@commercetools-uikit/spacings",
  "description": "A preset of all the UI-Kit spacings components.",
  "version": "10.39.1",
  "bugs": "https://github.com/commercetools/ui-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/ui-kit.git",
    "directory": "presets/spacings"
  },
  "homepage": "https://uikit.commercetools.com",
  "keywords": ["javascript", "design system", "react", "uikit"],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/spacings.cjs.js",
  "module": "dist/spacings.es.js",
  "files": ["dist"],
  "scripts": {
