{
  "name": "@commercetools-frontend/ui-kit",
  "description": "A preset of all the UI-Kit components.",
  "version": "10.39.1",
  "bugs": "https://github.com/commercetools/ui-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/ui-kit.git",
    "directory": "presets/ui-kit"
  },
  "homepage": "https://uikit.commercetools.com",
  "keywords": ["javascript", "design system", "react", "uikit"],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/ui-kit.cjs.js",
  "module": "dist/ui-kit.es.js",
  "files": ["dist", "materials", "i18n"],
  "scripts": {
    "prepare": "../../scripts/version.js replace",
    "prebuild": "rimraf dist",
    "build": "yarn build:bundles",
    "postbuild": "mkdir -p i18n/data && cp -R ../../design-system/materials materials && cp -R ../../packages/i18n/data i18n/",
    "build:bundles": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js -i ./index.js",
    "build:bundles:watch": "yarn build:bundles -w"
  },
  "dependencies": {
