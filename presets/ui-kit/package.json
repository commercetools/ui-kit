    "@commercetools-uikit/card": "10.39.1",
    "@commercetools-uikit/collapsible": "10.39.1",
    "@commercetools-uikit/collapsible-motion": "10.39.1",
    "@commercetools-uikit/collapsible-panel": "10.39.1",
