    "build:bundles:watch": "yarn build:bundles -w"
  },
  "dependencies": {
    "@babel/runtime": "7.12.1",
    "@babel/runtime-corejs3": "7.12.1",
    "@commercetools-uikit/accessible-button": "10.39.6",
    "@commercetools-uikit/design-system": "10.39.6",
    "@commercetools-uikit/flat-button": "10.39.6",
    "@commercetools-uikit/icon-button": "10.39.6",
    "@commercetools-uikit/link-button": "10.39.6",
    "@commercetools-uikit/primary-button": "10.39.6",
    "@commercetools-uikit/secondary-button": "10.39.6",
    "@commercetools-uikit/secondary-icon-button": "10.39.6"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-intl": "3.x || 4.x || 5.x",
    "react-router-dom": "5.x"
  }
