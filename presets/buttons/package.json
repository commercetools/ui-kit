{
  "name": "@commercetools-uikit/buttons",
  "description": "A preset of all the UI-Kit buttons components.",
  "version": "10.39.6",
  "bugs": "https://github.com/commercetools/ui-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/ui-kit.git",
    "directory": "presets/buttons"
  },
  "homepage": "https://uikit.commercetools.com",
  "keywords": [
    "javascript",
    "design system",
    "react",
    "uikit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/buttons.cjs.js",
  "module": "dist/buttons.es.js",
  "files": ["dist"],
  "scripts": {
    "prebuild": "rimraf dist",
